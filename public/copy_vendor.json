{
    "source": "https://cdnjs.cloudflare.com/ajax/libs/",
    "target": "js/vendor/",
    "format": "{source}{name}/{version}/{file}",
    "prefix": "/**! compression tag for ftp-deployment */\n\n",
    "renames": {
        "/\\.src\\./": "."
    },
    "plugins": [
        {
            "example": "https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.js",
            "name": "jquery",
            "version": "3.6.4",
            "files": [
                "jquery.js",
                "jquery.min.map"
            ]
        },
        {
            "name": "bootstrap",
            "version": "5.3.0-alpha3",
            "disabled": true,
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "target": "twitter-bootstrap",
            "files": [
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.js.map",
                "css/bootstrap.css",
                "css/bootstrap.css.map"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.4.0/css/all.css",
            "name": "font-awesome",
            "version": "6.4.0",
            "files": [
                "css/all.css",
                "webfonts/fa-brands-400.ttf",
                "webfonts/fa-brands-400.woff2",
                "webfonts/fa-regular-400.ttf",
                "webfonts/fa-regular-400.woff2",
                "webfonts/fa-solid-900.ttf",
                "webfonts/fa-solid-900.woff2"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/jquery-validation@1.19.5/dist/jquery.validate.js",
            "name": "jquery-validate",
            "version": "1.19.5",
            "files": [
                "jquery.validate.js",
                "additional-methods.js",
                "localization/messages_fr.js"
            ]
        },
        {
            "name": "highcharts",
            "version": "11.0.0",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/{file}",
            "files": [
                "highcharts.src.js",
                "highcharts.js.map"
            ]
        },
        {
            "display": "highcharts-accessibility",
            "name": "highcharts",
            "version": "11.0.0",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/modules/{file}",
            "target": "highcharts",
            "files": [
                "accessibility.src.js",
                "accessibility.js.map"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/html5sortable@0.13.3/dist/html5sortable.js",
            "name": "html5sortable",
            "version": "0.13.3",
            "files": [
                "html5sortable.js"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/mark.js@8.11.1/dist/jquery.mark.js",
            "display": "mark.js-jquery-mark",
            "name": "mark.js",
            "version": "8.11.1",
            "target": "jquery-mark",
            "files": [
                "jquery.mark.js"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/zxcvbn@4.4.2/dist/zxcvbn.js",
            "name": "zxcvbn",
            "version": "4.4.2",
            "files": [
                "zxcvbn.js",
                "zxcvbn.js.map"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/jquery-contextmenu@2.9.2/dist/jquery.contextMenu.js",
            "name": "jquery-contextmenu",
            "version": "2.9.2",
            "target": "contextmenu",
            "files": [
                "jquery.contextMenu.css",
                "jquery.contextMenu.js",
                "font/context-menu-icons.eot",
                "font/context-menu-icons.ttf",
                "font/context-menu-icons.woff",
                "font/context-menu-icons.woff2"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/clipboard@2.0.11/dist/clipboard.js",
            "name": "clipboard.js",
            "version": "2.0.11",
            "target": "clipboard",
            "files": [
                "clipboard.js"
            ]
        },
        {
            "display": "iconify",
            "name": "@iconify/iconify",
            "version": "1.0.7",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "target": "iconify",
            "disabled": true,
            "files": [
                "iconify.js"
            ]
        },
        {
            "name": "bootstrap-table",
            "version": "1.21.4",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "target": "bootstrap-table",
            "files": [
                "bootstrap-table.js",
                "bootstrap-table.css"
            ]
        },
        {
            "display": "bootstrap-table-defer-url",
            "name": "bootstrap-table",
            "version": "1.21.4",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/extensions/defer-url/{file}",
            "target": "bootstrap-table",
            "files": [
                "bootstrap-table-defer-url.js"
            ]
        },
        {
            "display": "bootstrap-table-custom-view",
            "name": "bootstrap-table",
            "version": "1.21.4",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/extensions/custom-view/{file}",
            "target": "bootstrap-table",
            "files": [
                "bootstrap-table-custom-view.js"
            ]
        },
        {
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "select2",
            "version": "4.1.0-rc.0",
            "files": [
                "js/select2.js",
                "js/i18n/fr.js",
                "css/select2.css"
            ]
        },
        {
            "display": "select2-bootstrap4-theme",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "@ttskch/select2-bootstrap4-theme",
            "version": "1.5.2",
            "target": "select2/css",
            "files": [
                "select2-bootstrap4.css"
            ]
        },
        {
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "currency-flags",
            "version": "4.0.7",
            "files": [
                "currency-flags.css"
            ]
        },
        {
            "example": "https://cdn.jsdelivr.net/npm/font-mfizz@2.4.1/dist/font-mfizz.css",
            "name": "font-mfizz",
            "version": "2.4.1",
            "files": [
                "font-mfizz.css",
                "font-mfizz.eot",
                "font-mfizz.svg",
                "font-mfizz.ttf",
                "font-mfizz.woff"
            ]
        },
        {
            "display": "bootstrap-dark",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "@forevolve/bootstrap-dark",
            "target": "bootstrap",
            "version": "4.0.0",
            "files": [
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.js.map",
                "css/bootstrap-dark.css",
                "css/bootstrap-light.css"
            ]
        }
    ],
    "file-style": [
        "bootstrap-dark.css",
        "bootstrap-light.css"
    ],
    "copy-style": {
        ".form-control:focus": ".field-valid",
        ".was-validated .form-control:invalid:focus, .form-control.is-invalid:focus": ".field-invalid",
        ".btn-success": ".toast-header-success",
        ".btn-warning": ".toast-header-warning",
        ".btn-danger": ".toast-header-danger",
        ".btn-info": ".toast-header-info",
        ".btn-primary": ".toast-header-primary",
        ".btn-secondary": ".toast-header-secondary",
        ".btn-dark": ".toast-header-dark"
    },
    "copy-entries": [
        {
            "search": ".dropdown-menu",
            "style": ".context-menu-list",
            "entries": [
                "background",
                "background-color",
                "border-radius",
                "color",
                "font-size"
            ]
        },
        {
            "search": ".dropdown-item",
            "style": ".context-menu-item",
            "entries": [
                "background-color",
                "color",
                "font-size",
                "font-weight",
                "padding",
                "padding-bottom",
                "padding-left",
                "padding-right",
                "padding-top",
                "margin"
            ]
        },
        {
            "search": ".dropdown-item:hover, .dropdown-item:focus",
            "style": ".context-menu-hover",
            "entries": [
                "background",
                "background-color",
                "color",
                "text-decoration"
            ]
        },
        {
            "search": ".dropdown-divider",
            "style": ".context-menu-separator",
            "entries": [
                "border-top",
                "margin"
            ]
        },
        {
            "search": ".dropdown-header",
            "style": ".context-menu-header",
            "entries": [
                "color",
                "display",
                "font-size",
                "margin-bottom",
                "white-space"
            ]
        },
        {
            "search": ".form-control",
            "style": ".simple-editor",
            "entries": [
                "color",
                "background-color"
            ]
        }
    ]
}
