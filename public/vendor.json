{
    "source": "https://cdnjs.cloudflare.com/ajax/libs/",
    "target": "js/vendor/",
    "format": "{source}{name}/{version}/{file}",
    "prefixes": {
        "js": "/**! compression tag for ftp-deployment */\n\n",
        "css": "/**! compression tag for ftp-deployment */\n\n"
    },
    "renames": {
        "\\.src\\.": "."
    },
    "plugins": [
        {
            "name": "jquery",
            "version": "3.6.1",
            "files": [
                "jquery.js",
                "jquery.min.map"
            ]
        },
        {
            "name": "bootstrap",
            "version": "4.6.2",
            "disabled": true,
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "target": "twitter-bootstrap",
            "files": [
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.js.map",
                "css/bootstrap.css",
                "css/bootstrap.css.map"
            ]
        },
        {
            "name": "font-awesome",
            "version": "6.2.0",
            "files": [
                "css/all.css",
                "webfonts/fa-brands-400.ttf",
                "webfonts/fa-brands-400.woff2",
                "webfonts/fa-regular-400.ttf",
                "webfonts/fa-regular-400.woff2",
                "webfonts/fa-solid-900.ttf",
                "webfonts/fa-solid-900.woff2",
                "webfonts/fa-v4compatibility.ttf",
                "webfonts/fa-v4compatibility.woff2"
            ]
        },
        {
            "name": "jquery-validate",
            "version": "1.19.5",
            "files": [
                "jquery.validate.js",
                "additional-methods.js",
                "localization/messages_fr.js"
            ]
        },
        {
            "name": "highcharts",
            "version": "10.3.1",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/{file}",
            "files": [
                "highcharts.src.js"
            ]
        },
        {
            "name": "highcharts",
            "version": "10.3.1",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/modules/{file}",
            "target": "highcharts",
            "files": [
                "accessibility.js",
                "accessibility.js.map"
            ]
        },
        {
            "name": "html5sortable",
            "version": "0.13.3",
            "files": [
                "html5sortable.js"
            ]
        },
        {
            "display": "mark.js-jquery-mark",
            "name": "mark.js",
            "version": "8.11.1",
            "target": "jquery-mark",
            "files": [
                "jquery.mark.js"
            ]
        },
        {
            "name": "zxcvbn",
            "version": "4.4.2",
            "files": [
                "zxcvbn.js",
                "zxcvbn.js.map"
            ]
        },
        {
            "name": "jquery-contextmenu",
            "version": "2.9.2",
            "target": "contextmenu",
            "files": [
                "jquery.contextMenu.css",
                "jquery.contextMenu.js",
                "font/context-menu-icons.eot",
                "font/context-menu-icons.ttf",
                "font/context-menu-icons.woff",
                "font/context-menu-icons.woff2"
            ]
        },
        {
            "name": "clipboard.js",
            "version": "2.0.11",
            "target": "clipboard",
            "files": [
                "clipboard.js"
            ]
        },
        {
            "name": "@iconify/iconify",
            "version": "1.0.7",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "target": "iconify",
            "disabled": true,
            "files": [
                "iconify.js"
            ]
        },
        {
            "name": "bootstrap-table",
            "version": "1.21.1",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "target": "bootstrap-table",
            "files": [
                "bootstrap-table.js",
                "bootstrap-table.css"
            ]
        },
        {
            "display": "bootstrap-table-defer-url",
            "name": "bootstrap-table",
            "version": "1.21.1",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/extensions/defer-url/{file}",
            "target": "bootstrap-table",
            "files": [
                "bootstrap-table-defer-url.js"
            ]
        },
        {
            "display": "bootstrap-table-custom-view",
            "name": "bootstrap-table",
            "version": "1.21.1",
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/extensions/custom-view/{file}",
            "target": "bootstrap-table",
            "files": [
                "bootstrap-table-custom-view.js"
            ]
        },
        {
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "select2",
            "version": "4.1.0-rc.0",
            "files": [
                "js/select2.js",
                "js/i18n/fr.js",
                "css/select2.css"
            ]
        },
        {
            "source": "https://cdn.jsdelivr.net/npm/@ttskch/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "select2-bootstrap4-theme",
            "version": "1.5.2",
            "target": "select2/css",
            "files": [
                "select2-bootstrap4.css"
            ]
        },
        {
            "name": "currency-flags",
            "version": "3.4.0",
            "files": [
                "currency-flags.css"
            ]
        },
        {
            "name": "font-mfizz",
            "version": "2.4.1",
            "files": [
                "font-mfizz.css",
                "font-mfizz.eot",
                "font-mfizz.svg",
                "font-mfizz.ttf",
                "font-mfizz.woff"
            ]
        },
        {
            "source": "https://cdn.jsdelivr.net/npm/@forevolve/",
            "format": "{source}{name}@{version}/dist/{file}",
            "name": "bootstrap-dark",
            "target": "bootstrap",
            "version": "2.1.1",
            "files": [
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.js.map",
                "css/bootstrap-dark.css",
                "css/bootstrap-light.css"
            ]
        },
        {
            "name": "admin-lte",
            "version": "3.2.0",
            "disabled": true,
            "files": [
                "js/adminlte.js",
                "js/adminlte.js.map",
                "css/adminlte.css",
                "css/adminlte.css.map"
            ]
        }
    ]
}
