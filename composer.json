{
	"name" : "bibi/calculation",
	"license" : "proprietary",
	"type" : "project",
	"homepage" : "https://www.bibi.nu",
	"description" : "Implementation of the calculation with Symfony 5.0.",
	"authors" : [{
			"name" : "Laurent Muller",
			"email" : "bibi@bibi.nu",
			"role" : "Administrator",
			"homepage" : "http://www.bibi.nu"
		}
	],
	"support" : {
		"email" : "calculation@bibi.nu",
		"source" : "https://github.com/laurentmuller/calculation"
	},
	"keywords" : [
		"symfony",
		"calculation"
	],
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/D-P-IT/FOSUserBundle.git"
		}
	],
	"require" : {
		"php" : "^7.4",
		"ext-ctype" : "*",
		"ext-iconv" : "*",
		"antishov/doctrine-extensions-bundle" : "^1.4",
		"beberlei/doctrineextensions" : "^1.2",
		"bjeavons/zxcvbn-php" : "^1.1",
		"fpdf/fpdf" : "^1.82",
		"friendsofsymfony/user-bundle" : "dev-bugfix",
		"fzaninotto/faker" : "^1.9",
		"google/recaptcha" : "^1.2",
		"imagine/imagine" : "^1.2",
		"ob/highcharts-bundle" : "^1.7",
		"robquistnl/simplehtmltotext" : "^1.1",
		"sensio/framework-extra-bundle" : "^5.1",
		"symfony/asset" : "5.1.*",
		"symfony/console" : "5.1.*",
		"symfony/dotenv" : "5.1.*",
		"symfony/expression-language" : "5.1.*",
		"symfony/flex" : "^1.3.1",
		"symfony/form" : "5.1.*",
		"symfony/framework-bundle" : "5.1.*",
		"symfony/http-client" : "5.1.*",
		"symfony/intl" : "5.1.*",
		"symfony/mailer" : "5.1.*",
		"symfony/mime" : "5.1.*",
		"symfony/monolog-bundle" : "^3.1",
		"symfony/orm-pack" : "*",
		"symfony/security-bundle" : "5.1.*",
		"symfony/serializer-pack" : "*",
		"symfony/swiftmailer-bundle" : "^3.1",
		"symfony/translation" : "5.1.*",
		"symfony/twig-pack" : "*",
		"symfony/validator" : "5.1.*",
		"symfony/web-link" : "5.1.*",
		"symfony/yaml" : "5.1.*",
		"twig/cssinliner-extra" : "^3.0",
		"twig/inky-extra" : "^3.0",
		"twig/markdown-extra" : "^3.0",
		"vich/uploader-bundle" : "1.*",
		"webinarium/datatables-bundle" : "^2.4"
	},
	"require-dev" : {
		"phpstan/extension-installer" : "~1.0",
		"phpstan/phpstan" : "^0.12",
		"phpstan/phpstan-deprecation-rules" : "~0.12",
		"phpstan/phpstan-doctrine" : "~0.12",
		"phpstan/phpstan-symfony" : "~0.12",
		"symfony/debug-pack" : "*",
		"symfony/maker-bundle" : "^1.0",
		"symfony/profiler-pack" : "*",
		"symfony/test-pack" : "*"
	},
	"config" : {
		"optimize-autoloader" : true,
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "2.*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php73" : "*",
		"symfony/polyfill-php72" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "5.1.*"
		}
	},
	"scripts" : {
		"app-cs-fixer" : "php-cs-fixer fix --diff"
	}
}
