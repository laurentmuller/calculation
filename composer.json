{
	"name" : "bibi/calculation",
	"type" : "project",
	"description" : "Implementation of the calculation with Symfony 5.4.",
	"keywords" : [
		"symfony",
		"calculation"
	],
	"homepage" : "https://github.com/laurentmuller/calculation",
	"license" : "proprietary",
	"authors" : [{
			"name" : "Laurent Muller",
			"email" : "bibi@bibi.nu",
			"homepage" : "https://www.bibi.nu",
			"role" : "Developer"
		}
	],
	"support" : {
		"email" : "calculation@bibi.nu",
		"issues" : "https://github.com/laurentmuller/calculation/issues",
		"source" : "https://github.com/laurentmuller/calculation"
	},
	"minimum-stability" : "stable",
	"prefer-stable" : true,
	"require" : {
		"php": "^8.1",
		"ext-ctype": "*",
		"ext-dom": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-intl": "*",
		"ext-libxml": "*",
		"ext-sqlite3": "*",
		"ext-zlib": "*",
		"beberlei/doctrineextensions": "^1.3",
		"bjeavons/zxcvbn-php": "^1.2",
		"doctrine/annotations": "^1.0",
		"doctrine/doctrine-bundle": "^2.5",
		"doctrine/orm": "^2.10",
		"ehymel/highcharts-bundle": "^1.7",
		"endroid/qr-code": "^4.2",
		"erusev/parsedown": "^1.7",
		"fakerphp/faker": "^1.14",
		"google/recaptcha": "^1.2",
		"imagine/imagine": "^1.2",
		"knplabs/knp-time-bundle": "^1.17",
		"phpoffice/phpspreadsheet": "^1.16",
		"robquistnl/simplehtmltotext": "^1.1",
		"sensio/framework-extra-bundle": "^6.1",
		"setasign/fpdf": "^1.8",
		"symfony/asset": "5.4.*",
		"symfony/console": "5.4.*",
		"symfony/dotenv": "5.4.*",
		"symfony/expression-language": "5.4.*",
		"symfony/flex": "^1.17|^2",
		"symfony/form": "5.4.*",
		"symfony/framework-bundle": "5.4.*",
		"symfony/http-client": "5.4.*",
		"symfony/intl": "5.4.*",
		"symfony/mailer": "5.4.*",
		"symfony/mime": "5.4.*",
		"symfony/monolog-bundle": "^3.1",
		"symfony/notifier": "5.4.*",
		"symfony/property-access": "5.4.*",
		"symfony/property-info": "5.4.*",
		"symfony/rate-limiter": "5.4.*",
		"symfony/runtime": "5.4.*",
		"symfony/security-bundle": "5.4.*",
		"symfony/serializer": "5.4.*",
		"symfony/string": "5.4.*",
		"symfony/translation": "5.4.*",
		"symfony/twig-bundle": "5.4.*",
		"symfony/validator": "5.4.*",
		"symfony/web-link": "5.4.*",
		"symfony/yaml": "5.4.*",
		"symfonycasts/reset-password-bundle": "^1.11",
		"symfonycasts/verify-email-bundle": "^1.7",
		"twig/cssinliner-extra": "^3.0",
		"twig/extra-bundle": "^3.0",
		"twig/inky-extra": "^3.0",
		"twig/markdown-extra": "^3.0",
		"twig/twig": "^3.0",
		"vich/uploader-bundle": "^1.15"
	},
	"require-dev" : {
		"bamarni/composer-bin-plugin": "^1.4",
		"phpunit/phpunit": "^9.5",
		"symfony/browser-kit": "5.4.*",
		"symfony/css-selector": "5.4.*",
		"symfony/debug-bundle": "5.4.*",
		"symfony/maker-bundle": "^1.0",
		"symfony/phpunit-bridge": "^6.0",
		"symfony/stopwatch": "5.4.*",
		"symfony/var-dumper": "5.4.*",
		"symfony/web-profiler-bundle": "5.4.*"
	},
	"replace" : {
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php56" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php72" : "*",
		"symfony/polyfill-php73" : "*",
		"symfony/polyfill-php74" : "*",
		"symfony/polyfill-php80" : "*",
		"symfony/polyfill-php81" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"config" : {
		"platform": {
            "php": "8.1.4"
        },
		"optimize-autoloader" : true,
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true,
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"bamarni/composer-bin-plugin": true,
			"symfony/runtime": true,
			"symfony/flex": true
		}
	},
	"extra" : {
		"symfony" : {
			"allow-contrib": true,
			"require": "5.4.*",
			"docker": false
		}
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"scripts" : {
		"bin" : "echo 'bin not installed'",
		"post-install-cmd" : "@composer bin all install --ansi",
		"post-update-cmd" : "@composer bin all update --ansi",
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		}
	}
}
