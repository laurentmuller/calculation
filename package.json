{
    "name": "calculation",
    "version": "1.1.0",
    "description": "Extends Bootstrap without rounded, themed popovers, tooltips and form-check.",
    "license": "MIT",
    "author": "bibi@bibi.nu",
    "type": "commonjs",
    "dependencies": {
        "@fortawesome/fontawesome-free": "latest",
        "@panzoom/panzoom": "latest",
        "bootstrap": "latest",
        "bootstrap-table": "latest",
        "clipboard": "latest",
        "currency-flags": "latest",
        "font-mfizz": "latest",
        "highcharts": "latest",
        "html-to-image": "latest",
        "html5sortable": "latest",
        "jquery": "latest",
        "jquery-contextmenu": "latest",
        "jquery-validation": "latest",
        "mark.js": "latest",
        "mermaid": "latest",
        "select2": "latest",
        "select2-bootstrap-5-theme": "latest",
        "zxcvbn": "latest"
    },
    "devDependencies": {
        "copy-files-from-to": "latest",
        "npm-run-all": "latest",
        "stylelint": "latest",
        "stylelint-config-standard":"latest"
    },
    "scripts": {
        "update": "npm update",
        "outdated": "npm outdated",
        "stylelint": "npx stylelint public/css/*.css",
        "clean": "del-cli public/build -f",
        "generate": "sass scss:public/build/bootstrap -q --no-source-map",
        "copy": "copy-files-from-to --config package-copy.json --silent",
        "build": "npm-run-all clean generate copy"
    },
    "jshintConfig": {
        "esversion": 9,
        "bitwise": true,
        "eqeqeq": true,
        "latedef": true,
        "strict": true,
        "asi": false
    }
}
