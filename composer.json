{
	"name": "bibi/calculation",
	"type": "project",
	"description": "Implementation of the calculation with Symfony 5.1.",
	"keywords": [
		"symfony",
		"calculation"
	],
	"homepage": "https://github.com/laurentmuller/calculation",
	"license": "proprietary",
	"authors": [{
			"name": "Laurent Muller",
			"email": "bibi@bibi.nu",
			"homepage": "https://www.bibi.nu",
			"role": "Developer"
		}
	],
	"support": {
		"email": "calculation@bibi.nu",
		"issues": "https://github.com/laurentmuller/calculation/issues",
		"source": "https://github.com/laurentmuller/calculation"
	},
	"require": {
		"php": "^7.4",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"beberlei/doctrineextensions": "^1.2",
		"bjeavons/zxcvbn-php": "^1.1",
		"doctrine/doctrine-bundle": "^2.1",
		"doctrine/doctrine-migrations-bundle": "^3.0",
		"doctrine/orm": "^2.7",
		"fpdf/fpdf": "^1.82",
		"fzaninotto/faker": "^1.9",
		"google/recaptcha": "^1.2",
		"imagine/imagine": "^1.2",
		"ob/highcharts-bundle": "^1.7",
		"phpoffice/phpspreadsheet": "^1.14",
		"robquistnl/simplehtmltotext": "^1.1",
		"sensio/framework-extra-bundle": "^5.1",
		"symfony/asset": "5.1.*",
		"symfony/console": "5.1.*",
		"symfony/dotenv": "5.1.*",
		"symfony/expression-language": "5.1.*",
		"symfony/flex": "^1.3.1",
		"symfony/form": "5.1.*",
		"symfony/framework-bundle": "5.1.*",
		"symfony/http-client": "5.1.*",
		"symfony/intl": "5.1.*",
		"symfony/mailer": "5.1.*",
		"symfony/mime": "5.1.*",
		"symfony/monolog-bundle": "^3.1",
		"symfony/security-bundle": "5.1.*",
		"symfony/serializer": "5.1.*",
		"symfony/translation": "5.1.*",
		"symfony/twig-bundle": "5.1.*",
		"symfony/validator": "5.1.*",
		"symfony/web-link": "5.1.*",
		"symfony/yaml": "5.1.*",
		"symfonycasts/reset-password-bundle": "^1.1",
		"symfonycasts/verify-email-bundle": "^1.0",
		"twig/cssinliner-extra": "^3.0",
		"twig/extra-bundle": "^3.0",
		"twig/inky-extra": "^3.0",
		"twig/markdown-extra": "^3.0",
		"twig/twig": "^3.0",
		"vich/uploader-bundle": "1.*",
		"webinarium/datatables-bundle": "^2.4"
	},
	"require-dev": {
		"phpstan/extension-installer": "~1.0",
		"phpstan/phpstan": "^0.12",
		"phpstan/phpstan-deprecation-rules": "~0.12",
		"phpstan/phpstan-doctrine": "~0.12",
		"phpstan/phpstan-symfony": "~0.12",
		"spaze/phpstan-disallowed-calls": "^1.0",
		"symfony/debug-pack": "*",
		"symfony/maker-bundle": "^1.0",
		"symfony/profiler-pack": "*",
		"symfony/test-pack": "*"
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php56": "*",
		"symfony/polyfill-php70": "*",
		"symfony/polyfill-php71": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php73": "*"
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"extra": {
		"symfony": {
			"allow-contrib": true,
			"require": "5.1.*"
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"app-cs-fixer": "php-cs-fixer fix --diff --diff-format udiff"
	}
}
