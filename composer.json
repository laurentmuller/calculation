{
    "name": "bibi/calculation",
    "type": "project",
    "description": "Implementation of the calculation with Symfony 6.x",
    "keywords": [
        "symfony",
        "calculation"
    ],
    "homepage": "https://github.com/laurentmuller/calculation",
    "license": "proprietary",
    "authors": [
        {
            "name": "Laurent Muller",
            "email": "bibi@bibi.nu",
            "homepage": "https://www.bibi.nu",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "calculation@bibi.nu",
        "issues": "https://github.com/laurentmuller/calculation/issues",
        "source": "https://github.com/laurentmuller/calculation"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-sqlite3": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "createnl/zxcvbn-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.0",
        "dukecity/doctrineextensions": "^2.0",
        "elao/enum": "^2.x-dev",
        "endroid/qr-code": "^5.0",
        "erusev/parsedown": "^1.0",
        "fakerphp/faker": "^1.0",
        "google/recaptcha": "^1.0",
        "imagine/imagine": "^1.0",
        "laurentmuller/highcharts-bundle": "^2.0",
        "phpoffice/phpspreadsheet": "^1.0",
        "phpoffice/phpword": "^1.0",
        "robquistnl/simplehtmltotext": "^1.0",
        "setasign/fpdf": "^1.0",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^2.0",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/rate-limiter": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfonycasts/reset-password-bundle": "^1.0",
        "symfonycasts/verify-email-bundle": "^1.0",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/markdown-extra": "^3.0",
        "twig/twig": "^3.0",
        "vich/uploader-bundle": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "friendsoftwig/twigcs": "^6.0",
        "orklah/psalm-insane-comparison": "^2.0",
        "orklah/psalm-not-empty": "^1.0",
        "orklah/psalm-strict-equality": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-phpunit": "^0.18",
        "psalm/plugin-symfony": "^5.0",
        "rector/rector": "^0.18",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/var-dumper": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "vimeo/psalm": "^5.0",
        "weirdan/doctrine-psalm-plugin": "^2.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70" : "*",
        "symfony/polyfill-php71" : "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "platform": {
            "php": "8.2.13"
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform-check": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/runtime": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*",
            "docker": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "php-fixer": "vendor/bin/php-cs-fixer fix --diff --dry-run",
        "php-psalm": "vendor/bin/psalm --config=psalm.xml",
        "php-stan": "vendor/bin/phpstan analyse --memory-limit 2G --configuration phpstan.neon",
        "php-rector": "vendor/bin/rector process --dry-run --config rector.php",
        "php-twigcs": "vendor/bin/twigcs --config .twig_cs.dist.php",
        "php-test": "phpunit",
        "php-all": [
            "@php-fixer",
            "@php-psalm",
            "@php-stan",
            "@php-rector",
            "@php-twigcs",
            "@php-test"
        ]
    },
    "scripts-descriptions": {
        "php-fixer": "Run PHP-CS-Fixer.",
        "php-psalm": "Run Pslam.",
        "php-stan": "Run PHPStan.",
        "php-rector": "Run Rector.",
        "php-twigcs": "Run Twigcs.",
        "php-test": "Run tests with PHPUnit.",
        "php-all": "Run all validation scripts."
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laurentmuller/HighchartsBundle"
        }
    ]
}
