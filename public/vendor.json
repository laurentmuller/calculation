{
	"source": "https://cdnjs.cloudflare.com/ajax/libs/",
	"target": "js/vendor/",
	"format": "{source}{name}/{version}/{file}",
	"bootswatch": "https://bootswatch.com/api/4.json",
	"prefixes": {
		"js": "/**! compression tag for ftp-deployment */\n\n",
		"css": "/**! compression tag for ftp-deployment */\n\n"
	},
	"renames": {
		"\\.src\\.": "."
	},
	"plugins": [
		{
			"name": "jquery",
			"version": "3.5.0",
			"files": [
				"jquery.js"
			]
		},
		{
			"name": "twitter-bootstrap",
			"version": "4.4.1",
			"files": [
				"js/bootstrap.bundle.js",
				"js/bootstrap.bundle.js.map",
				"css/bootstrap.css",
				"css/bootstrap.css.map"
			]
		},
		{
			"name": "font-awesome",
			"version": "5.13.0",
			"files": [
				"css/all.css",
				"webfonts/fa-brands-400.eot",
				"webfonts/fa-brands-400.svg",
				"webfonts/fa-brands-400.ttf",
				"webfonts/fa-brands-400.woff",
				"webfonts/fa-brands-400.woff2",
				"webfonts/fa-regular-400.eot",
				"webfonts/fa-regular-400.svg",
				"webfonts/fa-regular-400.ttf",
				"webfonts/fa-regular-400.woff",
				"webfonts/fa-regular-400.woff2",
				"webfonts/fa-solid-900.eot",
				"webfonts/fa-solid-900.svg",
				"webfonts/fa-solid-900.ttf",
				"webfonts/fa-solid-900.woff",
				"webfonts/fa-solid-900.woff2"
			]
		},
		{
			"name": "jquery-validate",
			"version": "1.19.1",
			"files": [
				"jquery.validate.js",
				"additional-methods.js",
				"localization/messages_fr.js"
			]
		},
		{
			"name": "summernote",
			"version": "0.8.16",
			"source": "https://cdn.jsdelivr.net/npm/",
			"format": "{source}{name}@{version}/dist/{file}",
			"files": [
				"summernote-bs4.css",
				"summernote-bs4.js",
				"summernote-bs4.js.map",
				"font/summernote.eot",
				"font/summernote.ttf",
				"font/summernote.woff",
				"font/summernote.woff2",
				"lang/summernote-fr-FR.js"
			]
		},
		{
			"name": "highcharts",
			"version": "8.0.4",
			"source": "https://cdn.jsdelivr.net/npm/",
			"format": "{source}{name}@{version}/{file}",
			"files": [
				"highcharts.src.js"
			]
		},
		{
			"name": "html5sortable",
			"version": "0.9.17",
			"files": [
				"html5sortable.js"
			]
		},
		{
			"name": "datatables",
			"version": "1.10.20",
			"source": "https://cdn.datatables.net/",
			"format": "{source}{version}/{file}",
			"files": [
				"js/jquery.dataTables.js",
				"js/dataTables.bootstrap4.js",
				"css/dataTables.bootstrap4.css"
			]
		},
		{
			"name": "keytable",
			"version": "2.5.1",
			"source": "http://cdn.datatables.net/",
			"target": "datatables",
			"files": [
				"js/dataTables.keyTable.js"
			]
		},
		{
			"name": "jquery.highlight",
			"version": "5",
			"source": "https://raw.githubusercontent.com/bartaz/sandbox.js/master",
			"format": "{source}/{file}",
			"target": "datatables/js",
			"files": [
				"jquery.highlight.js"
			]
		},
		{
			"name": "searchHighlight",
			"version": "1.10.20",
			"source": "https://cdn.datatables.net/plug-ins/",
			"format": "{source}{version}/features/{name}/{file}",
			"target": "datatables/js",
			"files": [
				"dataTables.searchHighlight.js"
			]
		},
		{
			"name": "conditionalPaging",
			"version": "1.10.20",
			"source": "https://cdn.datatables.net/plug-ins/",
			"format": "{source}{version}/features/{name}/{file}",
			"target": "datatables/js",
			"files": [
				"dataTables.conditionalPaging.js"
			]
		},
		{
			"name": "zxcvbn",
			"version": "4.4.2",
			"files": [
				"zxcvbn.js",
				"zxcvbn.js.map"
			]
		},
		{
			"name": "jquery-contextmenu",
			"version": "2.9.0",
			"target": "contextmenu",
			"files": [
				"jquery.contextMenu.css",
				"jquery.contextMenu.js",
				"font/context-menu-icons.eot",
				"font/context-menu-icons.ttf",
				"font/context-menu-icons.woff",
				"font/context-menu-icons.woff2"
			]
		},
		{
			"name": "clipboard.js",
			"version": "2.0.6",
			"target": "clipboard",
			"files": [
				"clipboard.js"
			]
		}
	]
}