{
    "sources": {
        "cdnjs": {
            "source": "https://cdnjs.cloudflare.com/ajax/libs/",
            "format": "{source}{name}/{version}/{file}",
            "versionUrl": "https://api.cdnjs.com/libraries/{name}?fields=version",
            "versionPaths": [
                "version"
            ]
        },
        "jsdelivr": {
            "source": "https://cdn.jsdelivr.net/npm/",
            "format": "{source}{name}@{version}/{file}",
            "target": "js/vendor/",
            "versionUrl": "https://data.jsdelivr.com/v1/package/npm/{name}",
            "versionPaths": [
                "tags",
                "latest"
            ]
        },
        "highcharts": {
            "source": "https://code.highcharts.com/",
            "format": "{source}{version}/{file}"
        }
    },
    "target": "vendor/",
    "prefixes": {
        "js": "/**! asset-version $version */\n\n",
        "css": "/**! asset-version $version */\n\n"
    },
    "plugins": [
        {
            "name": "jquery",
            "version": "3.7.1",
            "source": "cdnjs",
            "files": [
                "jquery.js"
            ]
        },
        {
            "name": "bootstrap",
            "version": "5.3.3",
            "source": "cdnjs",
            "files": [
                "js/bootstrap.bundle.js",
                "js/bootstrap.bundle.js.map",
                "css/bootstrap.css",
                "css/bootstrap.css.map"
            ]
        },
        {
            "name": "font-awesome",
            "version": "6.6.0",
            "source": "cdnjs",
            "files": [
                "css/all.css",
                "webfonts/fa-brands-400.ttf",
                "webfonts/fa-brands-400.woff2",
                "webfonts/fa-regular-400.ttf",
                "webfonts/fa-regular-400.woff2",
                "webfonts/fa-solid-900.ttf",
                "webfonts/fa-solid-900.woff2",
                "webfonts/fa-v4compatibility.ttf",
                "webfonts/fa-v4compatibility.woff2"
            ]
        },
        {
            "name": "jquery-validate",
            "version": "1.21.0",
            "source": "cdnjs",
            "remark": "use jquery-validation and jquery-validate target for jsdelivr.",
            "files": [
                "jquery.validate.js",
                "additional-methods.js"
            ]
        },
        {
            "name": "jquery-validate",
            "display": "jquery-validate:localization",
            "version": "1.21.0",
            "source": "cdnjs",
            "prefix": "localization/",
            "files": [
                "localization/messages_fr.js"
            ]
        },
        {
            "name": "highcharts",
            "version": "11.4.8",
            "source": "cdnjs",
            "files": [
                "highcharts.js"
            ]
        },
        {
            "name": "html5sortable",
            "version": "0.14.0",
            "source": "cdnjs",
            "files": [
                "html5sortable.js"
            ]
        },
        {
            "name": "mark.js",
            "version": "8.11.1",
            "source": "cdnjs",
            "target": "jquery-mark",
            "files": [
                "jquery.mark.js"
            ]
        },
        {
            "name": "zxcvbn",
            "version": "4.4.2",
            "source": "cdnjs",
            "files": [
                "zxcvbn.js"
            ]
        },
        {
            "name": "jquery-contextmenu",
            "version": "2.9.2",
            "source": "cdnjs",
            "files": [
                "jquery.contextMenu.css",
                "jquery.contextMenu.js",
                "font/context-menu-icons.eot",
                "font/context-menu-icons.ttf",
                "font/context-menu-icons.woff",
                "font/context-menu-icons.woff2"
            ]
        },
        {
            "name": "clipboard.js",
            "version": "2.0.11",
            "source": "cdnjs",
            "target": "clipboard",
            "files": [
                "clipboard.js"
            ]
        },
        {
            "name": "bootstrap-table",
            "version": "1.23.2",
            "source": "cdnjs",
            "files": [
                "bootstrap-table.js",
                "bootstrap-table.css"
            ]
        },
        {
            "name": "bootstrap-table",
            "display": "bootstrap-table:defer-url",
            "version": "1.23.2",
            "source": "cdnjs",
            "prefix": "extensions/defer-url/",
            "files": [
                "extensions/defer-url/bootstrap-table-defer-url.js"
            ]
        },
        {
            "name": "bootstrap-table",
            "display": "bootstrap-table:custom-view",
            "version": "1.23.2",
            "source": "cdnjs",
            "prefix": "extensions/custom-view/",
            "files": [
                "extensions/custom-view/bootstrap-table-custom-view.js"
            ]
        },
        {
            "name": "select2",
            "version": "4.1.0-rc.0",
            "source": "cdnjs",
            "files": [
                "js/select2.js",
                "js/i18n/fr.js",
                "css/select2.css"
            ]
        },
        {
            "name": "select2-bootstrap-5-theme",
            "version": "1.3.0",
            "source": "cdnjs",
            "target": "select2/css",
            "files": [
                "select2-bootstrap-5-theme.css"
            ]
        },
        {
            "name": "currency-flags",
            "version": "4.0.7",
            "source": "jsdelivr",
            "prefix": "dist/",
            "files": [
                "dist/currency-flags.css"
            ]
        },
        {
            "name": "font-mfizz",
            "version": "2.4.1",
            "source": "cdnjs",
            "files": [
                "font-mfizz.css",
                "font-mfizz.eot",
                "font-mfizz.svg",
                "font-mfizz.ttf",
                "font-mfizz.woff"
            ]
        },
        {
            "name": "html-to-image",
            "version": "1.11.11",
            "source": "cdnjs",
            "files": [
                "html-to-image.js"
            ]
        },
        {
            "name": "mermaid",
            "version": "11.0.2",
            "source": "cdnjs",
            "files": [
                "mermaid.js"
            ]
        },
        {
            "name": "svg-pan-zoom",
            "version": "3.6.1",
            "source": "jsdelivr",
            "prefix": "dist/",
            "target": "mermaid",
            "files": [
                "dist/svg-pan-zoom.js"
            ]
        }
    ]
}
